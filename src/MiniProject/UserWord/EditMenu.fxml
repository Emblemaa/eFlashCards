<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="510.0" prefWidth="540.0" style="-fx-background-color: #6495ED;" stylesheets="@../Style_Window.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MiniProject.UserWord.EditMenu_Controller">
   <children>
      <TableView fx:id="TBV_ListVocab" layoutY="83.0" prefHeight="345.0" prefWidth="540.0">
        <columns>
          <TableColumn fx:id="Word" editable="false" prefWidth="270.0" resizable="false" text="WORD" />
          <TableColumn fx:id="Definition" prefWidth="270.0" resizable="false" text="DEFINITION" />
        </columns>
      </TableView>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#back" styleClass="backButton" stylesheets="@../Style_Button.css" text="Back" />
      <Button layoutX="14.0" layoutY="436.0" mnemonicParsing="false" onAction="#addWord" prefHeight="60.0" prefWidth="200.0" styleClass="addButton" stylesheets="@../Style_Button.css" text="Add new word" textAlignment="CENTER" />
      <Button layoutX="326.0" layoutY="436.0" mnemonicParsing="false" onAction="#deleteWord" styleClass="addButton" stylesheets="@../Style_Button.css" text="Delete word" textAlignment="CENTER" />
      <CheckBox fx:id="Box" layoutX="202.0" layoutY="25.0" mnemonicParsing="false" onAction="#showUncheckedWords" styleClass="checkBox" text="Highlight incorrect words from test" textFill="WHITE">
         <stylesheets>
            <URL value="@../Style_CheckBox.css" />
            <URL value="@../Style_Text.css" />
         </stylesheets>
         <font>
            <Font size="18.0" />
         </font>
      </CheckBox>
   </children>
</AnchorPane>
